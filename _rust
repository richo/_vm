require File.expand_path('../lib/manager', __FILE__)
require File.expand_path('../contrib/zsh', __FILE__)

class Rust < Manager
  include PosixProxy
  if ENV['_VM_USE_ZSH']
    include Contrib::ZSH
  end

  name "rust"

  include Plugin::List
  # Needs to be included last of the main_case plugins
  # TODO : Setup LD_LOAD_PATH hacks for rlibs
  include Plugin::Set

  include Plugin::Use
  include Plugin::Reset
end

_rust = Rust.new
_rust.build

